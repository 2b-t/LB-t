{
  "name": "LB-t Docker Compose",
  "dockerComposeFile": [
    "../docker-compose.yml" // Alternatives: "../docker-compose-gui.yml", "../docker-compose-gui-nvidia.yml", "../docker-compose-nvidia.yml"
  ],
  "service": "lbt_docker",
	"workspaceFolder": "/code/lbt",
  "shutdownAction": "stopCompose",
  "extensions": [
    "zachflower.uncrustify",
    "ms-azuretools.vscode-docker",
    "ms-vscode.cpptools",
    "redhat.vscode-yaml",
    "streetsidesoftware.code-spell-checker",
    "twxs.cmake",
    "yzhang.markdown-all-in-one"
  ]
}